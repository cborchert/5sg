{
  "name": "svelte-ssg",
  "version": "1.0.0",
  "description": "simple static site generator for svelte",
  "main": "index.js",
  "license": "MIT",
  "author": "Christopher Borchert",
  "devDependencies": {
    "express": "^4.17.1",
    "nodemon": "^2.0.6",
    "remark": "^13.0.0",
    "remark-frontmatter": "^3.0.0",
    "remark-html": "^13.0.1",
    "remark-parse-frontmatter": "^1.0.1",
    "rimraf": "^3.0.2",
    "svelte": "^3.31.0",
    "unist-util-filter": "^2.0.3",
    "unist-util-find": "^1.0.2",
    "unist-util-visit": "^2.0.3"
  },
  "scripts": {
    "build": "node build.js OUTPUT_LEVEL=2",
    "build:quiet": "node build.js OUTPUT_LEVEL=0",
    "dev": "nodemon dev.js -e js,md,svelte,css,jpeg,jpg,png,gif,svg OUTPUT_LEVEL=2",
    "dev:quiet": "nodemon dev.js -e js,md,svelte,css,jpeg,jpg,png,gif,svg OUPUT_LEVEL=0"
  }
}
