{
  "name": "svelte-ssg",
  "version": "1.0.0",
  "description": "simple static site generator for svelte",
  "main": "index.js",
  "license": "MIT",
  "author": "Christopher Borchert",
  "dependencies": {
    "@rollup/plugin-node-resolve": "^11.1.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "fs-extra": "^9.0.1",
    "image-size": "^0.9.3",
    "rehype-katex": "^4.0.0",
    "rehype-parse": "^7.0.1",
    "rehype-preset-minify": "^5.1.0",
    "rehype-stringify": "^8.0.0",
    "remark": "^13.0.0",
    "remark-footnotes": "^3.0.0",
    "remark-frontmatter": "^3.0.0",
    "remark-gemoji": "^6.0.0",
    "remark-gfm": "^1.0.0",
    "remark-highlight.js": "^6.0.0",
    "remark-html": "^13.0.1",
    "remark-math": "^4.0.0",
    "remark-midas": "^6.0.0",
    "remark-parse-frontmatter": "^1.0.1",
    "remark-prism": "^1.3.4",
    "rimraf": "^3.0.2",
    "rollup": "^2.36.2",
    "rollup-plugin-svelte": "^7.0.0",
    "sharp": "^0.27.0",
    "svelte": "^3.31.0",
    "to-vfile": "^6.1.0",
    "unified": "^9.2.0",
    "unist-util-filter": "^2.0.3",
    "unist-util-find": "^1.0.2",
    "unist-util-visit": "^2.0.3",
    "vfile": "^4.2.1"
  },
  "devDependencies": {
    "eslint": "^7.17.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsdoc": "^31.0.5",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^26.6.3",
    "jsdoc": "^3.6.6",
    "mdsvex": "^0.8.9",
    "nodemon": "^2.0.6",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  },
  "scripts": {
    "build": "node build.js REPORTING_LEVEL=1",
    "build:quiet": "node build.js REPORTING_LEVEL=0",
    "build:explicit": "node build.js REPORTING_LEVEL=2",
    "dev": "nodemon build.js -e js,md,svelte,css,jpeg,jpg,png,gif,svg,html --ignore dist/ --ignore .5sg/ REPORTING_LEVEL=1 DEV=1",
    "dev:quiet": "nodemon build.js -e js,md,svelte,css,jpeg,jpg,png,gif,svg,html --ignore dist/ --ignore .5sg/ REPORTING_LEVEL=0 DEV=1",
    "dev:explicit": "nodemon build.js -e js,md,svelte,css,jpeg,jpg,png,gif,svg,html --ignore dist/ --ignore .5sg/ REPORTING_LEVEL=2 DEV=1",
    "test": "jest",
    "jsdoc": "jsdoc -c .jsdocrc **/*.js -R readme.md",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  }
}
